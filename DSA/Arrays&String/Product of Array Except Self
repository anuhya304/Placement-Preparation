class Solution(object):
    def productExceptSelf(self, nums):
        n= len(nums)
        prefix_prod=1
        postfix_prod=1
        result=[0]*n
        for i in range(n):
            result[i]=prefix_prod
            prefix_prod *=nums[i]
        for i in range(n-1,-1,-1):
            result[i]*= postfix_prod
            postfix_prod*=nums[i]
        return result 
